    <div id="curso" class="tab-pane fade">
      <h3>Mis Cursos</h3>
      <table class="table table-bordered">
      <thead>
        <tr>
          <th>Nombre</th>
          <th>Duracion</th>
          <th>Curso</th>
          <th>Compañeros</th>
          <th>Cambiar</th>
          <th>Eliminar</th>
        </tr>
      </thead>
      <tbody>
        <% @cursoList.each_index do |p| %>
        <tr>
          <td><%= @cursoList[p].nombre %></td>
          <td><%= @cursoList[p].duracion %> horas</td>
          <td><%= @cursoList[p].id_curso %></td>
          <td><%= link_to 'Ver Compañeros', "/people/inscritos?id="+@cursoList[p].id_curso %></td>
          <td><%= link_to 'Editar', edit_estudiante_inscripcion_path(@estudiante, @tmp[p]) %></td>
          <td><%= link_to 'Eliminar', estudiante_inscripcion_path(@tmp[p].estudiante, @tmp[p]), method: :delete, data: { confirm: 'Estas seguro?'}, :remote => true %></td>
        </tr>
        <% end %>
      </tbody>
