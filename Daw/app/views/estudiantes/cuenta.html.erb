        <h1 class="col-xl-10">Academia Politécnica de Nivelación</h1>
        <!--img id="logo" class="img-responsive" src="images/logonavidad.jpg" alt="logo"-->
        <%= link_to image_tag("logonavidad.jpg", id: "logo", class: "img-responsive") %>
      </div>
    </header>
        
  </div>
	<div class="row sticky-top" id="barralinks">
    	<nav class="navbar navbar-expand-xl navbar-dark col-xl-12">
        	<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#menuPrincipal">
         		<span class="navbar-toggler-icon"></span>
         	</button>
         	<div id="menuPrincipal" class="collapse navbar-collapse">
           		<ul class="list-inline navbar-nav mx-auto">
              		<li class="mr-5 list-inline-item nav-item active" ><h3 id="welcome">Bienvenido <%= current_estudiante.nombres %> <%= current_estudiante.apellidos %></h3></li>
            	</ul>
            	<ul class="nav navbar-nav navbar-right">
              		<li><h3><%= link_to("Logout", destroy_estudiante_session_path, class: "nav-link", method: "delete") %></h3></li>
            	</ul>
          	</div>
    	</nav>
	</div>

  <ul class="nav nav-tabs">
    <li class="active"><a data-toggle="tab" href="#perfil">Perfil</a></li>
    <li><a data-toggle="tab" href="#calificacion">Calificaciones</a></li>
    <li><a data-toggle="tab" href="#curso">Cursos</a></li>
  </ul>

  <div class="tab-content">
    <div id="perfil" class="tab-pane fade in active">
      <h3>Perfil de Usuario</h3>
      <table width=”650" id=”main” border=”0" cellpadding=”0" cellspacing=”20">
 		<tr>
 		<td class=”divider” colspan=”2" bgcolor=”#555555" height=”2"></td>
 		</tr>
 		<tr>
 		<td colspan=”2">
 		<p>Nombres: <%= current_estudiante.nombres %></p>
 		<p>Apellidos: <%= current_estudiante.apellidos %></p>
 		<p>Cedula: <%= current_estudiante.cedula %></p>
 		<p>Fecha de nacimiento: <%= current_estudiante.fecha_nac %></p>
 		<p>Email: <%= current_estudiante.email %></p>
 		<p>Direccion: <%= current_estudiante.domicilio %></p>
 		</td>
 		</tr>
 		<tr>
 		<td class=”divider” colspan=”2" bgcolor=”#555555" height=”2"></td>
 		</tr>
 		<tr>
 		<td id=”footer” colspan=”2" align=”center”>
 		<p>&copy;2019 Academia Politecnica de Nivelacion</p>
 		</td>
 		</tr>
 	  </table>
    <br>
    <a id ="button-edit" class="btn btn-primary" href=<%= edit_article_path(@estudiante) %> role="button">Editar Perfil</a>
