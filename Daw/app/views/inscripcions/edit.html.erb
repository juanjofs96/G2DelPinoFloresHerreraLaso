<h2>Cambiar de curso</h2>
<div class="text-center">
<%= form_with(model: [current_estudiante, @inscripcion], local: true) do |f| %>
 	<%= f.label :Estudiante  %>
    <% emp_array = [current_estudiante.id] %>
    <%= f.select :estudiante_id, options_for_select(emp_array) %> 
    <%= f.label :Curso  %>
    <% cursos = Curso.all.map { |n| [n.nombre, n.id] } %>
    <%= f.select :curso_id, options_for_select(cursos) %> 
 	<%= f.submit "Actualizar" %>
<% end %>
<div class="text-center">